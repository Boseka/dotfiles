[merge]
	conflictstyle = diff3
    tool = meld
[alias]
    st = status
    ci = commit
    co = checkout
    br = branch
    df = diff
    lg = log
	lgraph = log --graph --pretty=format:\"%h - %s\"
	unstage = reset HEAD --
    unmodify = checkout --
